<input type="button" onclick="location.href='/'" value="Registar producto">
<h1>Productos en Stock</h1>
<div class="product-container">
    {{#each products}}
    <div class="product-card">
        <p>{{this.name}}</p>
        <p>{{this.price}}$</p>
        <img src={{this.thumbnail}}>
    </div>
    {{/each}}
</div>